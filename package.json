{
	"name": "@nextnode/eslint-plugin",
	"version": "1.2.0",
	"description": "Typescript + React rules enforcement",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslintconfig",
		"next",
		"react",
		"typescript"
	],
	"license": "ISC",
	"author": "Walid M",
	"scripts": {
		"changeset": "changeset",
		"changeset:publish": "changeset publish",
		"changeset:version": "changeset version",
		"clean": "echo 'No build artifacts to clean for ESLint plugin'",
		"commitlint": "commitlint --edit",
		"format": "biome check --write .",
		"lint": "eslint --max-warnings=0 .",
		"lint-staged": "lint-staged",
		"prepare": "husky",
		"test": "echo 'No tests for ESLint plugin yet'"
	},
	"dependencies": {
		"@eslint/js": "^9.22.0",
		"@next/eslint-plugin-next": "^15.2.3",
		"@types/eslint__js": "^8.42.3",
		"@typescript-eslint/parser": "^8.4.0",
		"eslint": "^9.22.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import-x": "^4.1.1",
		"eslint-plugin-only-warn": "^1.1.0",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.0.0",
		"node": "^22.7.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.4",
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"better-sort-package-json": "^1.1.1",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2"
	},
	"peerDependencies": {
		"eslint": "^9.22.0"
	},
	"engines": {
		"node": ">= 20.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@10.11.0",
	"type": "module",
	"exports": {
		"./react": {
			"types": "./src/react.d.ts",
			"default": "./src/react.js"
		},
		"./next": {
			"types": "./src/next.d.ts",
			"default": "./src/next.js"
		},
		"./base": {
			"types": "./src/index.d.ts",
			"default": "./src/index.js"
		}
	}
}
